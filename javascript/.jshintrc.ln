// http://www.jshint.com/docs/options

{
  // Enforcing Options
  "forin": true,
  "immed": true,
  "latedef": "nofunc",
  "newcap": true,
  "noempty": true,
  "nonew": true,
  "quotmark": true,
  "undef": true,
  "unused": "vars",
  "trailing": true,

  // Relaxing Options
  "browser": true,
  "maxerr": 25
}
